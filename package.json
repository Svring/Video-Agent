{
  "name": "vite-reactts-electron-starter",
  "version": "0.6.0",
  "license": "MIT",
  "main": "dist-electron/index.js",
  "author": {
    "name": "Maximilian St√ºmpfl"
  },
  "description": "Starter using Vite 2+ React +Typescript + Electron for fast prototyping.",
  "keywords": [
    "vite",
    "react",
    "electron",
    "typescript"
  ],
  "repository": {
    "url": "https://github.com/lTimeless/vite-reactts-electron-starter"
  },
  "productName": "vite-reactts-electron-starter",
  "scripts": {
    "dev": "vite",
    "dev:electron": "npm run build:electron && electron .",
    "build": "npm run build:vite && npm run build:electron",
    "build:vite": "vite build",
    "build:electron": "tsc -p electron",
    "dist": "npm run build && electron-builder",
    "dist:win": "npm run build && electron-builder --win",
    "dist:mac": "npm run build && electron-builder --mac",
    "dist:linux": "npm run build && electron-builder --linux",
    "pack": "npm run build && electron-builder --dir",
    "clean": "rimraf dist main src/out",
    "type-check": "tsc",
    "lint": "eslint . --ext js,jsx,ts,tsx",
    "lint:fix": "eslint . --ext js,jsx,ts,tsx --fix"
  },
  "dependencies": {
    "@blocknote/ariakit": "latest",
    "@blocknote/core": "latest",
    "@blocknote/mantine": "latest",
    "@blocknote/react": "latest",
    "@blocknote/shadcn": "latest",
    "@emotion/react": "^11.13.3",
    "@emotion/styled": "^11.13.0",
    "@fontsource/roboto": "^5.1.0",
    "@material-tailwind/react": "^2.1.10",
    "@mui/icons-material": "^6.1.5",
    "@mui/material": "^6.1.5",
    "@nextui-org/react": "^2.4.8",
    "@types/node-fetch": "^2.6.11",
    "@vitejs/plugin-react": "^1.3.2",
    "allotment": "^1.20.2",
    "electron-is-dev": "^2.0.0",
    "framer-motion": "^11.11.10",
    "i18next": "^23.16.4",
    "node-fetch": "^2.7.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-i18next": "^14.1.3",
    "react-player": "^2.16.0",
    "vite": "^2.9.18",
    "vite-plugin-electron": "^0.28.8",
    "vite-plugin-electron-renderer": "^0.14.6"
  },
  "devDependencies": {
    "@types/node": "^17.0.45",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@typescript-eslint/eslint-plugin": "5.16.0",
    "@typescript-eslint/parser": "5.16.0",
    "autoprefixer": "^10.4.20",
    "electron": "^30.5.1",
    "electron-builder": "^24.13.3",
    "eslint": "8.11.0",
    "eslint-config-airbnb": "19.0.4",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-prettier": "4.0.0",
    "eslint-plugin-react": "^7.37.2",
    "postcss": "^8.4.47",
    "prettier": "2.6.0",
    "tailwindcss": "^3.4.14",
    "typescript": "^4.9.5"
  },
  "build": {
    "asar": true,
    "files": [
      "dist-electron",
      "dist-vite"
    ],
    "directories": {
      "buildResources": "resources"
    }
  }
}
